<table>
  <tr>
    <th>Title</th>
    <th>Address</th>
    <th>Description</th>
    <th>Price</th>
    <th>Creator</th>
    <th>Time Created</th>
  </tr>

  <% obj.each do |listing| %>
    <tr>
      <td><%= link_to("#{listing.title}", listing_path(listing.id)) %></td>
      <td><%= listing.address %></td>
      <td><%= listing.description %></td>
      <td><%= number_to_currency(listing.price) %></td>
      <td><%= link_to("#{listing.user.email}", user_path(listing.user.id)) %></td>
      <td><%= listing.created_at %></td>
    </tr>
  <% end %>
</table>
